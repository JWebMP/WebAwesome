# Environment variables for WebAwesome (see rules/generative/platform/secrets-config/env-variables.md)

# Asset configuration for WebAwesomePageConfigurator
WEB_AWESOME_BASE_PATH=/webawesome/
WEB_AWESOME_THEME_PATH=/webawesome/styles/themes/default.css
WEB_AWESOME_THEME_CLASS=wa-theme-default
WEB_AWESOME_THEME_PALETTE=wa-pallet-default
WEB_AWESOME_THEME_BRAND=wa-brand-blue
WEB_AWESOME_FA_KIT_CODE=

# Logging
LOG_LEVEL=INFO

# Testing and CI
SKIP_INTEGRATION_TESTS=false
ENABLE_DEBUG_LOGS=false
BROWSERSTACK_USERNAME=
BROWSERSTACK_ACCESS_KEY=
